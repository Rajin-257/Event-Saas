							<!-- Page start -->
							<div class="section padding-top-big">
								<div class="container">
									<div class="row">
										<div class="col-md-12">
											<h2 class="content__title">Check Tickit Validation</h2>
										</div>
										<div class="col-lg-4 mt-3">
											<h3 class="content__subtitle">The rapid handle injects into the bashful lift.</h3>
										</div>
									</div>
								</div>
							</div>
							
							<div class="section padding-top padding-bottom-big ">
								<div class="container">
									<div class="row">
										<div class="col-lg-6">
											<div class="otp">
												<label for="email-otp"> 
													<span class="error" id="err-email-otp">Please enter e-mail or Phone Number</span>
													<span class="error" id="err-emailvld-otp">E-mail or Phone is not in a valid format</span>
												</label>
												<input name="email-otp" id="email-otp" type="text" placeholder="E-Mail or Phone: *" />
												
												<button data-lang="en" id="tickit-vld-email"><span class="fa-long-arrow-right"></span></button>
											</div>
											<div class="otp-text">
												<p>* Make sure your entered e-mail or phone number is correct.</p>
											</div>
										</div>
				
										<div class="col-lg-6 mt-4 mt-lg-0">
											<div class="otp">
												<label for="otp"> 
													<span class="error" id="err-otp">Please Enter Otp</span>
													<span class="error" id="err-otpvld">Wrong Otp! Ensure You Entered Correct Phone Number Or email address </span>
												</label>
												<input name="otp" id="otp" type="text" placeholder="Your Otp: *"/>
												<button data-lang="en" id="otp-vld"><span class="fa-long-arrow-right"></span></button>
											</div>
											<div class="otp-text">
												<p>* Wait 30 Sec if otp not sent Enter Your Phone or email again</p>
											</div>
										</div>
									</div>
								</div>
							</div>

							<div class="section padding-bottom tickit">
								<div class="container">
									<div class="row">
										<div class="col-md-12">
											<!-- Start of Ticket Container -->
											<div class="ticket-container">
												<div class="ticket-inner">
													<div class="left-section">
														<div class="vinyl-record"></div>
														<h1 class="event-name">Funk Soul Music</h1>
														<div class="event-details">
															<div class="detail-row">
																<span class="label">Customer</span>
																<span class="value">John Smith</span>
															</div>
															<div class="detail-row">
																<span class="label">Phone</span>
																<span class="value">+1 (555) 123-4567</span>
															</div>
															<div class="detail-row">
																<span class="label">Date & Time</span>
																<span class="value">August 20th, 2024 • 6:00 PM</span>
															</div>
															<div class="detail-row">
																<span class="label">Venue</span>
																<span class="value">Anywhere Stadium, 123 ST, City Name</span>
															</div>
														</div>
													</div>
				
													<div class="right-section">
														<div class="price"><span>$</span>80</div>
														<div class="qr-code">
															<img src="/api/placeholder/140/140" alt="QR Code" width="140" height="140" />
														</div>
														<div class="admission-number">
															№ 0123 4567 8910
														</div>
														<div class="wavy-line"></div>
													</div>
												</div>
											</div>
											<!-- End of Ticket Container -->
										</div>
									</div>
								</div>
							</div>
				
							<div class="section padding-top padding-bottom-big background-dark footer">
								<div class="container">
									<div class="row">
										<div class="col-md-12">
											<% if (frontData.Logo) { %>
            		    						<img src="<%= frontData.Logo %>" >
            								<% } %>
											<h6 class="mt-5 mb-3">Get in touch:</h6>
											<p><%= frontData.email || "admin@gmail.com"  %></p>
											<p><%= frontData.phone || "+8801615928286"  %></p>
										</div>
										<div class="col-md-12 mt-5 mb-5">
											<div class="title-page-line"></div>
										</div>
										<div class="col-md-12">
